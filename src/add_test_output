#! /usr/remote/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x16db720 .scope module, "PC" "PC" 2 5;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 32 "nextPC"
    .port_info 2 /OUTPUT 32 "currPC"
o0x7f6cbdb03018 .functor BUFZ 1, C4<z>; HiZ drive
v0x16db8f0_0 .net "clk", 0 0, o0x7f6cbdb03018;  0 drivers
v0x16eb820_0 .var "currPC", 31 0;
o0x7f6cbdb03078 .functor BUFZ 32, C4<zzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzz>; HiZ drive
v0x16eb900_0 .net "nextPC", 31 0, o0x7f6cbdb03078;  0 drivers
E_0x16dc050 .event posedge, v0x16db8f0_0;
    .scope S_0x16db720;
T_0 ;
    %pushi/vec4 4194336, 0, 32;
    %store/vec4 v0x16eb820_0, 0, 32;
    %end;
    .thread T_0;
    .scope S_0x16db720;
T_1 ;
    %wait E_0x16dc050;
    %vpi_func 2 14 "$time" 64 {0 0 0};
    %cmpi/ne 0, 0, 64;
    %jmp/0xz  T_1.0, 4;
    %load/vec4 v0x16eb900_0;
    %store/vec4 v0x16eb820_0, 0, 32;
T_1.0 ;
    %jmp T_1;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "./PC.v";
